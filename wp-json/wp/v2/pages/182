page);\n            params.append('paged', paged);\n            displayLoading();\n\n            fetch(ds.ajax_url, {\n                method: 'POST',\n                credentials: 'same-origin',\n                headers: {\n                    'Content-Type': 'application\/x-www-form-urlencoded',\n                    'Cache-Control': 'no-cache',\n                },\n                body: params,\n            })\n                .then((response) => response.json())\n                .then((data) => {\n                    hideLoading();\n                    cs_cards.innerHTML += data.html_cards;\n                    cs_content.innerHTML += data.html_content;\n\n                    if( data.max_pages > data.paged ){\n                        paged = data.paged;\n                    }\n                    else{\n                        case_studies.getElementsByClassName('pagination-case-studies')[0].style.display = 'none';\n                    }\n                })\n                .catch((err) => console.log(err));\n        }\n\n        if( load_more.length > 0 ){\n            load_more[0].addEventListener('click', function (e) {\n                e.preventDefault();\n                getCaseStudies();\n            });\n        }\n    <\/script>\n    \n<\/section>\n\n\n\n\n<section class='wrapped-sections lazy has-gradient'>\n\n\t\n\t\n    \n\n\n<div class=\"spinner in-view-r\">\n\t<div class=\"spinner-inn\">\n\t\t<img\n                  data-srcset=\"\n                          https:\/\/www.digitalsilk.com\/wp-content\/themes\/digitalsilk\/assets\/_dist\/images\/gradient-bg-sm.png 1440w,\n                          https:\/\/www.digitalsilk.com\/wp-content\/themes\/digitalsilk\/assets\/_dist\/images\/gradient-bg.png 1660w\"\n                  data-src=\"https:\/\/www.digitalsilk.com\/wp-content\/themes\/digitalsilk\/assets\/_dist\/images\/gradient-bg-sm.png\"\n                  data-sizes=\"(min-width: 1441px) 1600px,\n                         1000px\"\n                